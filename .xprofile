# X11 compositor
#picom --experimental-backends -b &
~/.repo/picom/build/src/picom --experimental-backends -b &

# Pulse audio tray
pasystray &

# bluetooth applet
blueman-applet &

# network manager applet
nm-applet &

# drop down terminal
guake &

# redshift
redshift-gtk -l 24:84 -b 0.75:0.60 -o &

# Trayer as Standalone tray
trayer --edge top --align right --iconspacing 10 --height 20 --widthtype pixel --width 220 --margin 0 --transparent true --alpha 205 --SetDockType true  --SetPartialStrut true --tint 0x111111 &

